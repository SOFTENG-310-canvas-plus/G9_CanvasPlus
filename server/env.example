# Database connection details
SUPABASE_DATABASE_HOST=<your-database-host>
SUPABASE_DATABASE_PORT=<your-database-port>
SUPABASE_DATABASE_NAME=<your-database-name>
SUPABASE_DATABASE_USER=<your-database-username>
SUPABASE_DATABASE_PASSWORD=<your-database-password>

# JDBC connection string (auto-built from above variables, keep same format)
SUPABASE_DATABASE_URL="jdbc:postgresql://${SUPABASE_DATABASE_HOST}:${SUPABASE_DATABASE_PORT}/${SUPABASE_DATABASE_NAME}?sslmode=require"

# Supabase keys
SUPABASE_ANON_KEY=<your-supabase-anon-key>
SUPABASE_JWT_SIGNER=<your-supabase-jwt-signer>

# OpenAI API key (for AI integration)
OPENAI_API_KEY=<your-openai-api-key>